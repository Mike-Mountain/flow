{
  "projectType": "library",
  "root": "libs/container-core",
  "sourceRoot": "libs/container-core/src",
  "prefix": "ctr-core",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/container-core"
      ],
      "options": {
        "jestConfig": "libs/container-core/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/container-core/src/**/*.ts",
          "libs/container-core/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
